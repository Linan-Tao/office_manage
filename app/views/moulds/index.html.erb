<p id="notice"><%= notice %></p>

<h1>Listing Moulds</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moulds.each do |mould| %>
      <tr>
        <td><%= mould.code %></td>
        <td><%= mould.name %></td>
        <td><%= link_to 'Show', mould %></td>
        <td><%= link_to 'Edit', edit_mould_path(mould) %></td>
        <td><%= link_to 'Destroy', mould, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mould', new_mould_path %>
