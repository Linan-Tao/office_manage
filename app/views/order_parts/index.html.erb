<p id="notice"><%= notice %></p>

<h1>Listing Order Parts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_parts.each do |order_part| %>
      <tr>
        <td><%= link_to 'Show', order_part %></td>
        <td><%= link_to 'Edit', edit_order_part_path(order_part) %></td>
        <td><%= link_to 'Destroy', order_part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order part', new_order_part_path %>
