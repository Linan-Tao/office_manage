<%= simple_form_for(@produce_task) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :order %>
    <%= f.association :material_category %>
    <%= f.input :material_type %>
    <%= f.input :sequence %>
    <%= f.input :area %>
    <%= f.association :mix_task %>
    <%= f.input :mix_status %>
    <%= f.input :availability %>
    <%= f.input :work %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
