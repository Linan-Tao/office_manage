<%= simple_form_for(@delivery_plan,url: @delivery_plan.new_record? ? delivery_plans_path : delivery_plan_path(@delivery_plan)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :order, as: :select%>
    <%= f.input :produce_task_ids %>
    <%= f.input :number %>
    <%= f.input :fin_type %>
    <%= f.input :agency_fund %>
    <%= f.input :delivery_date %>
    <%= f.input :specify_logistic %>
    <%= f.association :logistic_provider %>
    <%= f.input :logistic_code %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-lg btn-block btn-primary', value: '提交' %>
  </div>
<% end %>
