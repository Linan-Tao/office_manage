<%= simple_form_for(@logistic_provider) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :director %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :qq %>
    <%= f.input :delivery_way %>
    <%= f.input :arrival_cycle %>
    <%= f.input :serve_rank %>
    <%= f.input :price_rank %>
    <%= f.input :secure_rank %>
    <%= f.input :credit_rank %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
