<%= simple_form_for(@logistic_provider,url: @logistic_provider.new_record? ? logistic_providers_path : logistic_provider_path(@logistic_provider)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :address %>
    <%= f.input :qq %>
    <%= f.input :director %>
    <%= f.input :director_phone %>
    <%= f.input :delivery_way %>
    <%= f.input :arrival_cycle %>
    <%= f.input :serve_rank %>
    <%= f.input :price_rank %>
    <%= f.input :secure_rank %>
    <%= f.input :credit_rank %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-lg btn-block btn-primary', value: '提交' %>
  </div>
<% end %>
