<%= simple_form_for(@out_storage) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :out_date %>
    <%= f.association :department %>
    <%= f.input :out_user %>
    <%= f.association :produce_task %>
    <%= f.input :item_id %>
    <%= f.input :item_type %>
    <%= f.input :number %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
