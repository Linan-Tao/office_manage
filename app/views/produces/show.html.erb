<div class="page-title">
  <h2><i class="fa fa-desktop color"></i>  生产管理 <small>详细</small>
  <span class="pull-right">
    <%= link_to('返回',produces_path, class: 'btn btn-lg btn-default') %>
  </span>
  </h2>
  <hr />
</div>
<div class="awidget full-width">
  <div class="awidget-head">
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <h3>生产详细</h3>
      </div>
    </div>
  </div>
  <div class="awidget-body">
    <table class="table table-hover table-striped table-bordered">
      <tbody>
        <tr>
          <th class="col-xs-2 text-center info">生产任务号</th>
          <td class="col-xs-10"><%= @produce.produce_task.order_id %></td>
        </tr>
        <tr>
          <th class="col-xs-2 text-center info">开始时间</th>
          <td class="col-xs-10"><%= @produce.start_time %></td>
        </tr>
        <tr>
          <th class="col-xs-2 text-center info">完成时间</th>
          <td class="col-xs-10"><%= @produce.end_time %></td>
        </tr>
        <tr>
          <th class="col-xs-2 text-center info">工序</th>
          <td class="col-xs-10"><%= @produce.work %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>