<div class="page-title">
  <h2><i class="fa fa-desktop color"></i>  混单管理 <small>详细</small>
  <span class="pull-right">
    <%= link_to('返回', :back, class: 'btn btn-lg btn-default') %>
  </span>
  </h2>
  <hr />
</div>
<div class="awidget full-width">
  <div class="awidget-head">
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <h3>混单详细</h3>
      </div>
    </div>
  </div>
  <div class="awidget-body">
    <table class="table table-hover table-striped table-bordered table-condensed">
      <tbody>
        <tr>
          <th class="col-xs-2 text-center info">大板数量</th>
          <td class="col-xs-10"><%= @mix_task.number %></td>
        </tr>
        <tr>
          <th class="col-xs-2 text-center info">利用率</th>
          <td class="col-xs-10"><%= @mix_task.availability %></td>
        </tr>
        <tr>
          <th class="col-xs-2 text-center info">混单文件</th>
          <td class="col-xs-10"><%= @mix_task.file_path %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>