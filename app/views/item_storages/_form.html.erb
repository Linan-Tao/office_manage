<%= simple_form_for(@item_storage) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :storage_date %>
    <%= f.input :receiver %>
    <%= f.input :purchase %>
    <%= f.input :item_id %>
    <%= f.input :item_type %>
    <%= f.input :brand %>
    <%= f.input :number %>
    <%= f.association :supplier %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
